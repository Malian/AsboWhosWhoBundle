    "description": "A bundle to add specific profile to user",
    "keywords": ["asbo", "bundle"],
    "homepage": "http://www.asbo.com",
    "license": "MIT",
    "authors": [
        {
            "name": "De Ron Malian",
            "email": "deronmalian@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/framework-bundle": ">=2.1,<2.4",
        "sensio/framework-extra-bundle": "*",
        "jms/security-extra-bundle": "1.5.*",
        "doctrine/doctrine-bundle": "*",
        "doctrine/orm": "*",
        "symfony/form": "~2.1",
        "symfony/security-bundle": "~2.1",
        "symfony/validator": "~2.1",
