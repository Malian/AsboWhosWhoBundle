<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="asbo.whoswho.entity.fra.class">Asbo\WhosWhoBundle\Entity\Fra</parameter>
        <parameter key="asbo.whoswho.entity.address.class">Asbo\WhosWhoBundle\Entity\Address</parameter>
        <parameter key="asbo.whoswho.entity.email.class">Asbo\WhosWhoBundle\Entity\Email</parameter>
        <parameter key="asbo.whoswho.entity.fraHasPost.class">Asbo\WhosWhoBundle\Entity\FraHasPost</parameter>
        <parameter key="asbo.whoswho.entity.phone.class">Asbo\WhosWhoBundle\Entity\Phone</parameter>
        <parameter key="asbo.whoswho.entity.diploma.class">Asbo\WhosWhoBundle\Entity\Diploma</parameter>
        <parameter key="asbo.whoswho.model.gmap_address.class">Asbo\WhosWhoBundle\Form\Type\GMapAddressType</parameter>
    </parameters>

    <services>

        <service id="form.type.gmap_address" class="%asbo.whoswho.model.gmap_Address.class%">
            <tag name="form.type" alias="gmap_address" />
        </service>

        <service id="asbo.whoswho.form.type.anno" class="Asbo\WhosWhoBundle\Form\Type\AnnoType">
            <tag name="form.type" alias="asbo_type_anno" />
        </service>

        <service id="asbo.whoswho.form.type.annotext" class="Asbo\WhosWhoBundle\Form\Type\AnnoTextType">
            <tag name="form.type" alias="asbo_type_annotext" />
        </service>

        <service id="asbo.whoswho.form.type.settings" class="Asbo\WhosWhoBundle\Form\Type\SettingsType">
            <tag name="form.type" alias="asbo_type_settings" />
        </service>

        <service id="asbo.whoswho.form.type.address" class="Asbo\WhosWhoBundle\Form\Type\AddressType">
            <argument>%asbo.whoswho.entity.address.class%</argument>
            <tag name="form.type" alias="asbo_type_address" />
        </service>

        <service id="asbo.whoswho.form.type.email" class="Asbo\WhosWhoBundle\Form\Type\EmailType">
            <argument>%asbo.whoswho.entity.email.class%</argument>
            <tag name="form.type" alias="asbo_type_email" />
        </service>

        <service id="asbo.whoswho.form.type.fraHasPost" class="Asbo\WhosWhoBundle\Form\Type\FraHasPostType">
            <argument>%asbo.whoswho.entity.fraHasPost.class%</argument>
            <tag name="form.type" alias="asbo_type_fraHasPost" />
        </service>

        <service id="asbo.whoswho.form.type.diploma" class="Asbo\WhosWhoBundle\Form\Type\DiplomaType">
            <argument>%asbo.whoswho.entity.diploma.class%</argument>
            <tag name="form.type" alias="asbo_type_diploma" />
        </service>

        <service id="asbo.whoswho.form.type.phone" class="Asbo\WhosWhoBundle\Form\Type\PhoneType">
            <argument>%asbo.whoswho.entity.phone.class%</argument>
            <tag name="form.type" alias="asbo_type_phone" />
        </service>

        <service id="asbo_whoswho.fra.form.factory" class="Asbo\WhosWhoBundle\Form\Factory\FormFactory">
            <argument type="service" id="form.factory" />
            <argument>asbo_whoswho_fra_edit</argument>
            <argument>asbo_type_fra</argument>
        </service>

        <service id="asbo_whoswho.form.factory.address" class="Asbo\WhosWhoBundle\Form\Factory\FormFactory">
            <argument type="service" id="form.factory" />
            <argument>asbo_whoswho_address</argument>
            <argument>asbo_type_address</argument>
        </service>

        <service id="asbo_whoswho.form.eventlistener.editfra" class="Asbo\WhosWhoBundle\Form\EventListener\EditFraListener" public="false" />

        <service id="asbo.whoswho.form.type.fra" class="Asbo\WhosWhoBundle\Form\Type\FraType">
            <argument>%asbo.whoswho.entity.fra.class%</argument>
            <argument type="service" id="asbo_whoswho.form.eventlistener.editfra" />
            <tag name="form.type" alias="asbo_type_fra" />
        </service>

    </services>

</container>
