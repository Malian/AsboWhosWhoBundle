<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services/services-1.0.xsd">


    <parameters>
        <parameter key="asbo_whoswho.fra_manager.class">Asbo\WhosWhoBundle\Entity\FraManager</parameter>
        <parameter key="asbo_whoswho.fra_has_user_manager.class">Asbo\WhosWhoBundle\Entity\FraHasUserManager</parameter>
        <parameter key="asbo_whoswho.fra_has_post_manager.class">Asbo\WhosWhoBundle\Entity\FraHasPostManager</parameter>
        <parameter key="asbo_whoswho.routing.urlGenerator.class">Asbo\WhosWhoBundle\Routing\UrlGenerator</parameter>
        <parameter key="asbo_whoswho.translation_domain">AsboWhosWhoBundle</parameter>
        <parameter key="asbo_whoswho.security.owner_voter.class">Asbo\WhosWhoBundle\Security\OwnerVoter</parameter>
    </parameters>

    <services>

        <service id="asbo_whoswho.routing.urlGenerator" class="%asbo_whoswho.routing.urlGenerator.class%">
            <argument type="service" id="router" />
        </service>

        <service id="asbo_whoswho.security.owner_voter" class="%asbo_whoswho.security.owner_voter.class%" public="false">
            <tag name="security.voter" />
        </service>

    </services>
</container>
