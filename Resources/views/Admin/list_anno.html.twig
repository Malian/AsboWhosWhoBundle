{% extends 'SonataAdminBundle:CRUD:base_list_field.html.twig' %}

{% block field %}
    {%- if value is empty -%}
        -
    {%- else -%}
        {% if value.timestamp is defined %}
            {{ value|date('Y') | DateToAnno }}
        {% else %}
            {{ value|DateToAnno }}
        {% endif %}
    {%- endif -%}
{% endblock %}
