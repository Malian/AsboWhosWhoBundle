{% if form.vars.errors is not empty%}
    <div class="alert alert-error">
        {{ form_errors(form) }}
    </div>
{% endif %}

<form novalidate action="{{ path('asbo_whoswho_frahaspost_create', {'slug': fra.slug}) }}" {{ form_enctype(form) }} method="POST">
    {{ form_rest(form) }}
    <input type="submit" value="Ajouter" class="btn btn-success" />
</form>
